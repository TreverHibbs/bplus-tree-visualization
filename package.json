{
  "name": "bplus-tree-visualization",
  "version": "1.0.0",
  "description": "a visualization learning tool for b-plus-trees",
  "main": "index.js",
  "scripts": {
    "test": "TESTBUILD=true ./node_modules/.bin/webpack b --config webpack.dev.js && ./node_modules/.bin/mocha test-dist/bundle.js",
    "watch-test": "concurrently \"TESTBUILD=true ./node_modules/.bin/webpack b -w\" \"./node_modules/.bin/mocha -w test-dist/bundle.js\"",
    "watch-test-build": "TESTBUILD=true ./node_modules/.bin/webpack b -w --config webpack.dev.js",
    "watch-mocha": "./node_modules/.bin/mocha -w test-dist/bundle.js",
    "start-test-server": "TESTBUILD=true npx webpack serve --open --config webpack.dev.js",
    "watch-ts": "./node_modules/.bin/tsc -w",
    "watch-scss": "./node_modules/.bin/sass --watch dist/css/main.scss dist/css/main.css",
    "start-server": "webpack serve --open --config webpack.dev.js",
    "watch-webpack": "node_modules/.bin/webpack b -w",
    "start": "concurrently \"npm:watch-scss\" \"npm:start-server\"",
    "start-dev": "concurrently -p \"{time}-{name}\" -t \"ss\" \"npm:watch-scss\" \"npm:start-server\" \"npm:start-test-server\"",
    "build": "webpack --config webpack.prod.js",
    "deploy": "gh-pages -d dist",
    "deploy-gh-actions": "gh-pages -d dist -u \"github-actions-bot <support+actions@github.com>\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TreverHibbs/bplus-tree-visualization.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TreverHibbs/bplus-tree-visualization/issues"
  },
  "homepage": "https://github.com/TreverHibbs/bplus-tree-visualization#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/d3-shape": "^2.1.0",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "gh-pages": "^3.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mocha": "^8.4.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@types/d3-hierarchy": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "d3-hierarchy": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "webpack": "^5.37.1"
  }
}
